{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "hooks": {
        "pre-install": [
          "bash ./eas-build-pre-install.sh"
        ],
        "post-install": [
          "bash ./eas-build-post-install.sh"
        ]
      },
      "image": "latest"
    }
  },
  "submit": {
    "production": {}
  }
}
